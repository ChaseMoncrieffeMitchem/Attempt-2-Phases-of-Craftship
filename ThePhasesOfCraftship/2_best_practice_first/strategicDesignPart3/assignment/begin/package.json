{
  "name": "dddforum",
  "version": "1.0.0",
  "description": "root directory for frontend backend and shared dependencies",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start:dev:front": "npm run start:dev --workspace=@dddforum/frontend",
    "start:dev:back": "npm run start:dev --workspace=@dddforum/backend",
    "test:e2e:front": "npm run test:e2e --workspace=@dddforum/frontend",
    "test:e2e:back": "npm run test:e2e --workspace=@dddforum/backend",
    "clean": "npm run clean --workspaces --if-present",
    "build": "npm run build --workspaces --if-present",
    "lint": "npm run lint --workspaces --if-present"
  },
  "devDependencies": {
    "@types/cucumber": "^7.0.0",
    "@types/jest": "^29.5.13",
    "@types/node": "^18.16.19",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "envalid": "^8.0.0",
    "eslint": "^8.53.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "jest-cucumber": "^3.0.2",
    "rimraf": "^3.0.2",
    "run-script-os": "^1.1.6",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.6.3"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "nodemailer": "^6.9.13"
  }
}
